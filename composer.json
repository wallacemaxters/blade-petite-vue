{
    "name": "wallacemaxters/blade-petite-vue",
    "description": "A small wrapper to usage petite-vue with Laravel Blade",
    "type": "library",
    "license": "MIT",
	"scripts": {
		"test" : "./vendor/bin/phpunit"
	},
    "autoload": {
        "psr-4": {
            "WallaceMaxters\\BladePetiteVue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "wallacemaxters",
            "email": "wallacemaxters@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "orchestra/testbench": "^8.5"
    },
    "require": {
        "illuminate/support": "^9.0 || ^10.0",
        "illuminate/view": "^9.0 || ^10.0"
    }
}
